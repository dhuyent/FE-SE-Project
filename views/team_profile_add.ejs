<section class="team-profile-add">
    <h2 class="team-profile-add__heading">Hồ sơ đội bóng</h2>
    <form class="team-form">
        <div class="form-group">
            <label for="team-name">Tên đội bóng</label>
            <input type="text" id="team-name" name="team-name" placeholder="Nhập tên đội bóng">
        </div>

        <div class="form-group">
            <label for="home-stadium">Sân nhà</label>
            <input type="text" id="home-stadium" name="home-stadium" placeholder="Nhập tên sân nhà">
        </div>

        <div class="form-group">
            <label for="coach-name">Huấn luyện viên</label>
            <input type="text" id="coach-name" name="coach-name" placeholder="Nhập tên huấn luyện viên">
        </div>

        <div class="form-group">
            <label for="number-of-players">Số cầu thủ</label>
            <input type="number" id="number-of-players" name="number-of-players" placeholder="Nhập số cầu thủ">
        </div>

        <div class="team-actions">
            <button type="submit" class="save-btn">Lưu thông tin</button>
            <a href="#" class="edit-link">Chỉnh sửa</a>
        </div>
    </form>
</section>